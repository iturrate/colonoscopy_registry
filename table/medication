select
        ORDER_MED_ID,
        ORDER_MED.PAT_ID,
        ORDER_MED.PAT_ENC_CSN_ID,
        ORDER_MED.ORDER_INST as ORDERING_TIME,
        ORDER_MED.ORDER_START_TIME ORDER_START_TIME,
        ORDER_MED.ORDER_END_TIME ORDER_END_TIME,
        ORDER_MED.DISCON_TIME DISCONTINUE_TIME,
        ORDER_MED.SIG,
        ORDER_MED.HV_DISCRETE_DOSE DISCRETE_DOSE,
        ZC_MED_UNIT.NAME as DOSE_UNIT,
        ORDER_MED.LASTDOSE,
        IP_FREQUENCY.FREQ_NAME FREQUENCY,
        ZC_ADMIN_ROUTE.NAME ROUTE,
        ORDER_MED.MEDICATION_ID,
        CLARITY_MEDICATION.NAME as MEDICATION_NAME,
        CLARITY_MEDICATION.GENERIC_NAME,
        CLARITY_MEDICATION.THERA_CLASS_C,
        ZC_THERA_CLASS.NAME as THERA_CLASS,
        CLARITY_MEDICATION.PHARM_CLASS_C,
        ZC_PHARM_CLASS.NAME as PHARM_CLASS,
        CLARITY_MEDICATION.PHARM_SUBCLASS_C,
        ZC_PHARM_SUBCLASS.NAME as PHARM_SUBCLASS,
        ZC_ORDER_CLASS.NAME as ORDER_CLASS,
        ZC_ORDER_STATUS.NAME as ORDER_STATUS,
        ZC_ORDERING_MODE.NAME as ORDERING_MODE
from
        clr_medications.ORDER_MED
        inner join DATABASE.patient on patient.PAT_ID = ORDER_MED.PAT_ID
        left join clr_medications.CLARITY_MEDICATION on ORDER_MED.MEDICATION_ID = CLARITY_MEDICATION.MEDICATION_ID
        left join clr_clarity_reference.ZC_ADMIN_ROUTE on ORDER_MED.MED_ROUTE_C = ZC_ADMIN_ROUTE.MED_ROUTE_C
        left join clr_clarity_reference.ZC_THERA_CLASS on CLARITY_MEDICATION.THERA_CLASS_C = ZC_THERA_CLASS.THERA_CLASS_C
        left join clr_clarity_reference.ZC_PHARM_CLASS on CLARITY_MEDICATION.PHARM_CLASS_C = ZC_PHARM_CLASS.PHARM_CLASS_C
        left join clr_clarity_reference.ZC_PHARM_SUBCLASS on CLARITY_MEDICATION.PHARM_SUBCLASS_C = ZC_PHARM_SUBCLASS.PHARM_SUBCLASS_C
        left join clr_clarity_reference.ZC_ORDER_STATUS on ORDER_MED.ORDER_STATUS_C = ZC_ORDER_STATUS.ORDER_STATUS_C
        left join clr_clarity_reference.ZC_ORDER_CLASS on ORDER_MED.ORDER_CLASS_C = ZC_ORDER_CLASS.ORDER_CLASS_C
        left join clr_clarity_reference.ZC_MED_UNIT on ORDER_MED.HV_DOSE_UNIT_C = ZC_MED_UNIT.DISP_QTYUNIT_C
        left join clr_clarity_reference.ZC_ORDERING_MODE on ZC_ORDERING_MODE.ORDERING_MODE_C = ORDER_MED.ORDERING_MODE_C
        left join clr_medications.IP_FREQUENCY on IP_FREQUENCY.FREQ_ID = ORDER_MED.HV_DISCR_FREQ_ID
where
        ( ORDER_MED.ORDER_STATUS_C is null or ORDER_MED.ORDER_STATUS_C != 4 )
        and ORDERING_DATE is not null
