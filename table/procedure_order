select
        ORDER_PROC.ORDER_PROC_ID,
        ORDER_PROC.PAT_ID,
        ORDER_PROC.PAT_ENC_CSN_ID,
        ORDER_PROC.PROC_ID,
        ORDER_PROC.DESCRIPTION,
        ORDER_PROC.ORDERING_DATE ,
        ZC_ORDER_STATUS.NAME as ORDER_STATUS,
        ZC_ORDERING_MODE.NAME as ORDERING_MODE
from
        clr_orders.ORDER_PROC
        inner join DATABASE.patient on patient.PAT_ID = ORDER_PROC.PAT_ID
        left join clr_clarity_reference.ZC_ORDER_STATUS on ORDER_PROC.ORDER_STATUS_C = ZC_ORDER_STATUS.ORDER_STATUS_C
        left join clr_orders.ORDER_PROC_3 on ORDER_PROC_3.ORDER_ID = ORDER_PROC.ORDER_PROC_ID
        left join clr_clarity_reference.ZC_ORDERING_MODE on ZC_ORDERING_MODE.ORDERING_MODE_C = ORDER_PROC_3.ORDERING_MODE_C
