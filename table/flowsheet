select
  IP_FLWSHT_MEAS.FSD_ID,
  IP_FLWSHT_MEAS.LINE,
  PAT_ENC_HSP.PAT_ID,
  PAT_ENC_HSP.PAT_ENC_CSN_ID,
  IP_FLWSHT_MEAS.FLO_MEAS_ID,
  IP_FLO_GP_DATA.FLO_MEAS_NAME NAME,
  IP_FLO_GP_DATA.DISP_NAME DISPLAY_NAME,
  IP_FLWSHT_MEAS.MEAS_VALUE OBSERVATION_VALUE,
  IP_FLWSHT_MEAS.MEAS_COMMENT OBSERVATION_COMMENT,
  IP_FLO_GP_DATA.UNITS UNIT,
  IP_FLWSHT_MEAS.RECORDED_TIME RECORDED_TIME
from
  clr_encounters.PAT_ENC_HSP
  inner join DATABASE.patient on patient.PAT_ID = PAT_ENC_HSP.PAT_ID
  INNER JOIN clr_clinical_documentation.IP_FLWSHT_REC on IP_FLWSHT_REC.INPATIENT_DATA_ID = PAT_ENC_HSP.INPATIENT_DATA_ID
  left join clr_clinical_documentation.IP_FLWSHT_MEAS on IP_FLWSHT_MEAS.FSD_ID = IP_FLWSHT_REC.FSD_ID
  left join clr_clinical_documentation.IP_FLO_GP_DATA on IP_FLO_GP_DATA.FLO_MEAS_ID = IP_FLWSHT_MEAS.FLO_MEAS_ID
