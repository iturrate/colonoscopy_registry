select
                SOCIAL_HX.PAT_ID,
                SOCIAL_HX.HX_LNK_ENC_CSN PAT_ENC_CSN_ID,
                SOCIAL_HX.PAT_ENC_DATE_REAL SOCIAL_HX_ID,
                ZC_SMOKING_TOB_USE.NAME as TOBACCO_USE,
                ZC_SMOKELESS_TOB_U.NAME as SMOKELESS_TOBACCO_USE,
                ZC_ALCOHOL_USE.NAME ALCOHOL_USE,
                --ZC_OB_PREG_HX.NAME ILLICIT_DRUG_USE,
                IV_DRUG_USER_YN IV_DRUG_USE,
                ZC_SEXUALLY_ACTIVE.NAME SEXUALLY_ACTIVE,
                SOCIAL_HX.CONTACT_DATE
from
                clr_patient_history.SOCIAL_HX
                inner join DATABASE.patient on patient.PAT_ID = SOCIAL_HX.PAT_ID
                left join clr_clarity_reference.ZC_SMOKELESS_TOB_U on SOCIAL_HX.SMOKELESS_TOB_USE_C = ZC_SMOKELESS_TOB_U.SMOKELESS_TOB_U_C
                left join clr_clarity_reference.ZC_SMOKING_TOB_USE on SOCIAL_HX.SMOKING_TOB_USE_C = ZC_SMOKING_TOB_USE.SMOKING_TOB_USE_C
                left join clr_clarity_reference.ZC_ALCOHOL_USE on ZC_ALCOHOL_USE.ALCOHOL_USE_C = SOCIAL_HX.ALCOHOL_USE_C
                --left join clr_clarity_reference.ZC_OB_PREG_HX on ZC_OB_PREG_HX.OB_PREG_HX_C = SOCIAL_HX.ILL_DRUG_USER_C
                left join clr_clarity_reference.ZC_SEXUALLY_ACTIVE on ZC_SEXUALLY_ACTIVE.SEXUALLY_ACTIVE_C = SOCIAL_HX.SEXUALLY_ACTIVE_C
