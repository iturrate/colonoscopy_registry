select
  IP_LDA_NOADDSINGLE.pat_enc_csn_id,
  IP_LDA_NOADDSINGLE.pat_id,
  IP_LDA_NOADDSINGLE.PLACEMENT_INSTANT,
  IP_LDA_NOADDSINGLE.REMOVAL_INSTANT,
  ZC_LINES_GROUP.NAME
from
  clr_clinical_documentation.IP_LDA_NOADDSINGLE
  left join clr_clinical_documentation.IP_FLO_LDA_TYPES on IP_FLO_LDA_TYPES.ID = IP_LDA_NOADDSINGLE.FLO_MEAS_ID and
  IP_FLO_LDA_TYPES.CONTACT_DATE_REAL = IP_LDA_NOADDSINGLE.LDA_GROUP_CDR
  left join clr_clarity_reference.ZC_LINES_GROUP on ZC_LINES_GROUP.LINES_GROUP_C = ip_flo_lda_types.lda_type_ot_c
  inner join DATABASE.encounter on encounter.pat_enc_csn_id = IP_LDA_NOADDSINGLE.pat_enc_csn_id
