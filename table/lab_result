select
        ORDER_PROC.ORDER_PROC_ID,
        ORDER_RESULTS.LINE,
        ORDER_PROC.PAT_ID,
        ORDER_PROC.PAT_ENC_CSN_ID,
        ORDER_PROC.PROC_ID,
        ORDER_PROC.DESCRIPTION ORDER_DESCRIPTION,
        ORDER_PROC.ORDERING_DATE,
        ORDER_RESULTS.RESULT_TIME RESULT_TIME,
        ORDER_RESULTS.COMPONENT_ID,
        CLARITY_COMPONENT.NAME COMPONENT_NAME,
        ORDER_RESULTS.ORD_NUM_VALUE NUMERIC_VALUE,
        ORDER_RESULTS.ORD_VALUE RAW_VALUE,
        ORDER_RESULTS.REFERENCE_UNIT,
        ZC_RESULT_FLAG.NAME as RESULT_FLAG,
        ZC_RESULT_STATUS.NAME as RESULT_STATUS,
        ZC_LAB_STATUS.NAME as LAB_STATUS,
        CLARITY_COMPONENT.LOINC_CODE
from
        clr_orders.ORDER_RESULTS
        left join clr_orders.ORDER_PROC on ORDER_RESULTS.ORDER_PROC_ID = ORDER_PROC.ORDER_PROC_ID
        inner join DATABASE.patient on patient.PAT_ID = ORDER_PROC.PAT_ID
        left join clr_orders.CLARITY_COMPONENT on ORDER_RESULTS.COMPONENT_ID = CLARITY_COMPONENT.COMPONENT_ID
        left join clr_clarity_reference.ZC_RESULT_FLAG on ORDER_RESULTS.RESULT_FLAG_C = ZC_RESULT_FLAG.RESULT_FLAG_C
        left join clr_clarity_reference.ZC_RESULT_STATUS on ORDER_RESULTS.RESULT_STATUS_C = ZC_RESULT_STATUS.RESULT_STATUS_C
        left join clr_clarity_reference.ZC_LAB_STATUS on ORDER_RESULTS.LAB_STATUS_C = ZC_LAB_STATUS.LAB_STATUS_C
where
        ORDER_RESULTS.RESULT_STATUS_C in ( 3, 4 )  -- final, , corrected
        and ORDER_RESULTS.LAB_STATUS_C in ( 3, 5 ) -- final, edited final
